#!/bin/bash


## PACKAGE 
PACKAGE="${PACKAGE}scx-scheds"

if [[ ! -z $( ip a | grep "wl" ) ]] ;then
    PACKAGE="${PACKAGE} wireless-regdb"
fi

## CONFIGS
if [[ -d "$appdir/library/install/kernels/lqx/config" ]]; then
    cp -fr "$appdir/library/install/kernels/lqx/config/"* "$appdir/config/define/"
fi