#!/bin/bash

## PACKAGE 
PACKAGE="${PACKAGE}linux-lts scx-scheds"

if [[ ! -z $( ip a | grep "wl" ) ]] ;then
    PACKAGE="${PACKAGE} wireless-regdb"
fi

## CONFIGS
if [[ -d "$appdir/library/installs/kernels/lts/config" ]]; then
    cp -fr "$appdir/library/installs/kernels/lts/config/"* "$appdir/config/define/"
fi