#!/bin/bash

## PACKAGE 
PACKAGE="${PACKAGE}linux-zen scx-scheds"

if [[ ! -z $( ip a | grep "wl" ) ]] ;then
    PACKAGE="${PACKAGE} wireless-regdb"
fi


## CONFIGS
if [[ -d "$appdir/library/installs/kernels/zen/config" ]]; then
    cp -fr "$appdir/library/installs/kernels/zen/config/"* "$appdir/config/define/"
fi