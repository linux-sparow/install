#!/bin/bash

## PACKAGE
PACKAGE="${PACKAGE} dnsmasq bluez-utils networkmanager openssh"

## DAEMONS
RDAEMON="${RDAEMON} sshd systemd-timesyncd.service dnsmasq NetworkManager"


## CONFIGS
if [[ -d "$appdir/library/install/netdev/nmt/config" ]]; then
    cp -fr "$appdir/library/install/netdev/nmt/config/"* "$appdir/config/define/"
fi