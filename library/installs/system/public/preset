#!/bin/bash

## PACKAGE 
PACKAGE="${PACKAGE} base base-devel linux-firmware sof-firmware mkinitcpio mesa ttf-droid kitty-terminfo bash-completion git wget aria2 unzip neovim"


## DAEMONS
RDAEMON="${RDAEMON} update.timer"




## CONFIGS
if [[ -d "$appdir/library/system/public/config" ]]; then
    cp -fr "$appdir/library/system/public/config/"* "$appdir/config/define/"
fi